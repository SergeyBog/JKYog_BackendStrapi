    },
    "collections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::collection.collection",
      "mappedBy": "topic"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "topic"
    },
    "videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::video.video",
      "mappedBy": "topic"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
